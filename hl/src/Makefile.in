top_srcdir=@top_srcdir@
top_builddir=../..
srcdir=@srcdir@
@COMMENCE@

## Add `-I.' to the C preprocessor flags.
CPPFLAGS=-I. -I$(srcdir) -I$(top_builddir)/src -I$(top_srcdir)/src  -I$(top_srcdir)/hl/src @CPPFLAGS@

## This is our main target, but also remove the settings file when cleaning.
LIB=libhdf5_hl.la
LIBHDF5=$(top_builddir)/src/libhdf5.la
CLEAN=

## Source and object files for the library (lexicographically)...
LIB_SRC=H5LT.c H5TB.c H5IM.c H5DS.c

LIB_OBJ=$(LIB_SRC:.c=.lo)

## Temporary files
MOSTLYCLEAN=

## Public header files (to be installed)...
PUB_HDR=H5IM.h H5LT.h H5TB.h H5DS.h

## Other header files (not to be installed)...
PRIVATE_HDR=

@CONCLUDE@

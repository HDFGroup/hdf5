	@rm -f perf_meta$(EXEEXT)
	$(LINK) $(perf_meta_OBJECTS) $(perf_meta_LDADD) $(LIBS)
zip_perf$(EXEEXT): $(zip_perf_OBJECTS) $(zip_perf_DEPENDENCIES) 
	@rm -f zip_perf$(EXEEXT)
	$(LINK) $(zip_perf_OBJECTS) $(zip_perf_LDADD) $(LIBS)


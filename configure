$as_echo "$lt_cv_archive_cmds_need_lc_FC" >&6; }
      archive_cmds_need_lc_FC=$lt_cv_archive_cmds_need_lc_FC
      ;;
    esac
  fi
  ;;
esac






























































    { $as_echo "$as_me:${as_lineno-$LINENO}: checking dynamic linker characteristics" >&5
